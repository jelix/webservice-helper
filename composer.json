    "homepage": "http://www.jool.nl/jool-tech-zone/webservice-helper/",
    "license": "LGPL-2.1",
    "support": {
        "source" : "https://github.com/jelix/webservice-helper"
    },
    "authors": [
        {
            "name": "David Kingma",
            "email": "david@jool.nl"
        },
        {
            "name": "Laurent Jouanneau",
            "email": "laurent@jelix.org"
        },
        {
            "name": "Sylvain de Vathaire",
            "email": "s.de.vathaire@neov.net"
        }
    ],
    "require": {
        "php": ">=5.2"
    },
    "require-dev": {
        "phpunit/phpunit":"4.3.*"
